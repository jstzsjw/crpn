}
layer {
  name: "rpn_prob_tr"
  type: "Softmax"
  bottom: "rpn_score_tr"
  top: "rpn_prob_tr"
}
layer {
  name: "rpn_prob_br"
  type: "Softmax"
  bottom: "rpn_score_br"
  top: "rpn_prob_br"
}
layer {
  name: "rpn_prob_bl"
  type: "Softmax"
  bottom: "rpn_score_bl"
  top: "rpn_prob_bl"
}
layer {
  name: "rpn_label_tl"
  type: "Python"
  bottom: "conv_rpn"
  bottom: "im_info"
  bottom: "gt_boxes"
  top: "rpn_label_tl"
  top: "rpn_label_tr"
  top: "rpn_label_br"
  top: "rpn_label_bl"
  python_param {
    module: "rpn.labelmap_layer"
    layer: "LabelMapLayer"
  }
}
layer {
  name: "loss_rpn_tl"
  type: "BalancedSoftmaxWithLoss"
  bottom: "rpn_score_tl"
  bottom: "rpn_label_tl"
  top: "loss_rpn_tl"
  propagate_down: true
  propagate_down: false
  loss_param {
    ignore_label: -1
    normalize: true
  }
}
layer {
  name: "loss_rpn_tr"
  type: "BalancedSoftmaxWithLoss"
  bottom: "rpn_score_tr"
  bottom: "rpn_label_tr"
