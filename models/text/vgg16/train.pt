    normalize: true
  }
}
layer {
  name: "loss_rpn_bl"
  type: "BalancedSoftmaxWithLoss"
  bottom: "rpn_score_bl"
  bottom: "rpn_label_bl"
  top: "loss_rpn_bl"
  propagate_down: true
  propagate_down: false
  loss_param {
